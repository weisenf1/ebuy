<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <filter>
  	<filter-name>AdminFilter</filter-name>
  	<filter-class>com.weisen.ebuy2.filter.AdminFilter</filter-class>
  </filter>
  <filter-mapping>
  	<filter-name>AdminFilter</filter-name>
  	<url-pattern>/AdminData.jsp</url-pattern>
  </filter-mapping>
  <servlet>
  	<servlet-name>AdminServlet</servlet-name>
  	<display-name>AdminServlet</display-name>
  	<description></description>
  	<servlet-class>com.weisen.ebuy2.servlet.AdminServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>AdminDataServlet</servlet-name>
  	<display-name>AdminDataServlet</display-name>
  	<description></description>
  	<servlet-class>com.weisen.ebuy2.servlet.AdminDataServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>AdminServlet</servlet-name>
  	<url-pattern>/AdminServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>AdminDataServlet</servlet-name>
  	<url-pattern>/AdminDataServlet</url-pattern>
  </servlet-mapping>
</web-app>
